#!/usr/bin/env node

var program = require('commander');
var chalk = require('chalk');

program
.parse(process.argv);

if (program.args.indexOf('build') >= 0) {
  require('../build')({
    cwd: process.cwd(),
  });

  return;
}

if (program.args.indexOf('dev') >= 0) {
  require('../dev')({
    cwd: process.cwd(),
  });
  
  return;
}
